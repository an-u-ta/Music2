@model IEnumerable<Music.Models.Artist>

@{
    ViewBag.Title = "Index";
}

<h2>Artists</h2>

<p class="btn btn-warning btn-lg">
    @Html.ActionLink("Add Artist", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
       
    </tr>
}

</table>
